#! /usr/bin/env python

import sys
from hlpr_manipulation_utils import manipulator
from hlpr_manipulation_utils.manipulator import *

if __name__ == '__main__':
  rospy.init_node('arm_tuck', anonymous=True)
  
  print "sys.argv[1] = ", str(sys.argv[1])
  
  if (len(sys.argv) > 1 and (str(sys.argv[1])=='left' or str(sys.argv[1])=='right')):
  	arm = Arm(arm_prefix = str(sys.argv[1]))
  else: 
  	arm = Arm()
  	
  arm.lower_tuck()
